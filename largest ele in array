import java.util.*;
public class array{
    public static void main(String[] args){
        Scanner ip = new Scanner(System.in);
        int n = ip.nextInt();
        int[] arr = new int[n]; //this is how we get input of the array from user
        for(int i=0;i<n;i++){
            arr[i]=ip.nextInt();
        }
        int max = arr[0];
        for(int i =0;i<n;i++){
            if(arr[i]>max){
                max=arr[i];
            }
        }
        System.out.println("The largest element in the array is:"+ max);
    }
}
